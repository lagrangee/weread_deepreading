{
  "name": "weread_deepreading",
  "version": "1.0.0",
  "description": "微信读书深度阅读助手是一个Chrome浏览器扩展，为微信读书用户提供智能阅读辅助功能，包括文本解释、内容消化、兼听等功能。",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run clean && webpack --mode development --watch",
    "start": "npm run dev",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "npm run build && cd dist && zip -r ../weread-deepreading-v$npm_package_version.zip .",
    "analyze": "webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagrangee/weread_deepreading.git"
  },
  "keywords": [
    "chrome-extension",
    "weread",
    "ai-assistant"
  ],
  "author": "lagrangee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lagrangee/weread_deepreading/issues"
  },
  "homepage": "https://github.com/lagrangee/weread_deepreading#readme",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "core-js": "^3.35.0",
    "regenerator-runtime": "^0.14.1"
  }
}
